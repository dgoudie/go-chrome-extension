{
    "name": "go/chrome-extension",
    "version": "1.9",
    "description": "URL Shortener",
    "action": {
        "default_popup": "popup/index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "declarativeNetRequest",
        "storage"
    ],
    "host_permissions": [
        "*://go/*"
    ],
    "manifest_version": 3,
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    }
}