{
    "name": "go/chrome-extension",
    "version": "1.8",
    "description": "URL Shortener",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_popup": "popup/index.html"
    },
    "permissions": [
        "webRequestBlocking",
        "webRequest",
        "storage",
        "*://go/*"
    ],
    "manifest_version": 2,
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    }
}